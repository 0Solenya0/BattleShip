<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socketHandler.view.RegistrationView">
    <children>
        <TextField fx:id="txtUsername" layoutX="215.0" layoutY="167.0" />
        <PasswordField fx:id="txtPassword" layoutX="215.0" layoutY="200.0" />
        <Label fx:id="lblUsername" layoutX="137.0" layoutY="171.0" text="Username" />
        <Label fx:id="lblPassword" layoutX="140.0" layoutY="204.0" text="Passowrd" />
        <Button fx:id="btnLogin" layoutX="264.0" layoutY="246.0" mnemonicParsing="false" onAction="#btnRegisterClicked" text="Register" />
        <Button fx:id="btnRegister" layoutX="488.0" layoutY="14.0" mnemonicParsing="false" onAction="#btnLoginClicked" prefHeight="24.0" prefWidth="98.0" text="Login" />
      <Label fx:id="lblErr" layoutX="188.0" layoutY="282.0" text="Error" textFill="RED" />
    </children>
</AnchorPane>
